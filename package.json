{
  "name": "front-products",
  "version": "1.0.0",
  "description": "challenge",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config webpack.dev.config.js",
    "start:stubs": "npm run stubs | npm start",
    "stubs": "stubby --watch --quiet --data stubby.json",
    "lint": "eslint --max-warnings 0 src test",
    "test:unit": "mochapack --webpack-config webpack.test.config.js --recursive 'test/unit'",
    "coverage": "cross-env NODE_ENV=coverage nyc --check-coverage=true --temp-dir=./node_modules/.cache/nyc_output --reporter=text npm run test:unit",
    "test:unit:watch": "npm run test:unit -- --watch",
    "security:auth": "snyk auth $SNYK_TOKEN",
    "security:test": "npm run security:auth && snyk test",
    "security:monitor": "npm run security:auth && snyk monitor",
    "ci": "npm run lint && npm run test:unit && npm run coverage"
  },
  "nyc": {
    "statements": 90,
    "lines": 90,
    "functions": 90,
    "branches": 90,
    "include": [
      "src/**/*.js"
    ],
    "instrument": false,
    "sourceMap": false
  },
  "author": "fparra",
  "license": "ISC",
  "dependencies": {
    "@webcomponents/custom-elements": "^1.3.1",
    "@webcomponents/shadydom": "^1.7.0",
    "axios": "0.19.0",
    "classnames": "2.2.6",
    "prop-types": "15.7.2",
    "ramda": "0.26.1",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-intl": "2.9.0"
  },
  "devDependencies": {
    "@babel/core": "7.5.0",
    "@babel/plugin-proposal-class-properties": "7.5.0",
    "@babel/polyfill": "7.7.0",
    "@babel/preset-env": "7.5.2",
    "@babel/preset-react": "7.0.0",
    "babel-eslint": "9.0.0",
    "babel-loader": "8.0.6",
    "chai": "4.2.0",
    "chai-enzyme": "1.0.0-beta.1",
    "clean-webpack-plugin": "3.0.0",
    "compression-webpack-plugin": "^6.1.1",
    "copy-webpack-plugin": "^6.0.3",
    "cross-env": "5.2.0",
    "css-loader": "3.0.0",
    "dirty-chai": "2.0.1",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "eslint": "5.16.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-react-hooks": "1.5.0",
    "express-static-gzip": "2.0.3",
    "file-loader": "4.0.0",
    "html-webpack-plugin": "^4.5.0",
    "istanbul-instrumenter-loader": "3.0.1",
    "less": "3.9.0",
    "less-loader": "5.0.0",
    "mocha": "^8.2.1",
    "mochapack": "^2.0.6",
    "null-loader": "3.0.0",
    "nyc": "14.1.1",
    "path": "0.12.7",
    "sinon": "7.3.2",
    "sinon-chai": "3.3.0",
    "snyk": "1.223.0",
    "stubby": "^5.0.0",
    "style-loader": "^0.23.1",
    "svg-inline-loader": "0.8.0",
    "webpack": "4.37.0",
    "webpack-cli": "3.3.5",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.4.0",
    "webpack-node-externals": "1.7.2"
  }
}
